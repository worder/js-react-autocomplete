{
  "name": "vda-js-react-autocomplete",
  "version": "0.1.0",
  "private": true,
  "main": "dist/Autocomplete.js",
  "dependencies": {
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "co": "4.x",
    "lodash": "4.x",
    "prop-types": "15.x",
    "react-themeable": "^1.1.0"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-transform-class-properties"
    ],
    "env": {
      "production": {
        "plugins": [
          "babel-plugin-transform-react-remove-prop-types"
        ]
      }
    }
  },
  "scripts": {
    "install": "npm run build",
    "build": "rm -rf dist && mkdir dist && BABEL_ENV=\"production\" babel src -d dist --copy-files --verbose",
    "build-demo": "cd demo && npm run build",
    "watch": "concurrently \"babel src -d dist -w --copy-files\" \"cd demo && npm run watch\""
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.17.1"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  }
}
